
namespace WebPedidos.WSClasses
{
    public class ClienteResumido
    {
        int     codigo;
        string  nome;
        
        public int Codigo
        {
            get { return codigo; }
            set { codigo = value; }
        }

        public string Nome
        {
            get { return nome; }
            set { nome = value; }
        }
        
        public ClienteResumido() { }
        public ClienteResumido(int codigo, string nome)
        {
            Codigo = codigo;
            Nome = nome;
        }
    }
}
